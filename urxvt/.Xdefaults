! scrollback buffer lines - 65535 is max on most machines (64 is default)
URxvt*saveLines: 12000

URxvt*background: #000000
URxvt*foreground: #ffffff

URxvt*letterSpace: -1

!URxvt*font: xft:Droid Sans Mono:style=Regular:pixelsize=13:antialias=false
! URxvt*boldFont: xft:Droid Sans:style=bold:pixelsize=13:antialias=false

URxvt*font: xft:Monaco:style=Regular:pixelsize=13:antialias=false

! URxvt*font: xft:Inconsolata:style=Regular:pixelsize=16:antialias=true

! urxvt.font:             9x15bold,\
!                         -misc-fixed-bold-r-normal--15-140-75-75-c-90-iso10646-1,\
!                         -misc-fixed-medium-r-normal--15-140-75-75-c-90-iso10646-1, \
!                         [codeset=JISX0208]xft:Kochi Gothic, \
!                         xft:Bitstream Vera Sans Mono:autohint=true, \
!                         xft:Code2000:antialias=false
! urxvt.boldFont:         -xos4-terminus-bold-r-normal--14-140-72-72-c-80-iso8859-15
! urxvt.italicFont:       xft:Bitstream Vera Sans Mono:italic:autohint=true
! urxvt.boldItalicFont:   xft:Bitstream Vera Sans Mono:bold:italic:autohint=true

Emacs*useXIM: false


URxvt.cursorBlink: true
URxvt.cursorUnderline: true
URxvt*cursorColor: green
!URxvt*pointerBlank: true

! Transparency 
URxvt.transparent:   true
URxvt.tintColor:     #673AB7
URxvt.shading:       50

! Typing ibus in rxvt-unicode
! Follow here: https://wiki.archlinux.org/index.php/IBus#rxvt-unicode
URxvt.imLocale: en_US.UTF-8
URxvt.inputMethod: ibus
URxvt.preeditType: OverTheSpot,None


URxvt.selection.pattern-0: ([[:word:]]+)

! scrollbar - true (default) or false
URxvt*scrollBar: false


! scrollbar position - left=false (default) or right=true
URxvt*scrollBar_right: true

!scrollbar style - rxvt (default), plain, next, or xterm
URxvt*scrollstyle: plain




! Perl extension
! URxvt.perl-lib: /usr/lib/urxvt/perl/
! URxvt.perl-ext-common: keyboard-select,url-select,clipboard,font-size
! !Key board select
! URxvt.keysym.M-Escape: perl:keyboard-select:activate
! URxvt.keysym.M-s: perl:keyboard-select:search


! ! url select
! URxvt.keysym.M-u: perl:url-select:select_next
! URxvt.url-select.launcher: /bin/ffd
! URxvt.url-select.underline: true




! black
URxvt*color0  : #000000
URxvt*color8  : #555753
! red
URxvt*color1  : #CC0000
URxvt*color9  : #EF2929
! green
URxvt*color2  : #4E9A06
!color10: #8AE234
URxvt*color10 : #55FF55
! yellow
URxvt*color3  : #C4A000
! color11:#FCE94F
URxvt*color11 : #FFFF55
! blue
URxvt*color4  : #3465A4
URxvt*color12 : #739FCF
! magenta
URxvt.color5  : #AA00AA
URxvt.color13 : #FF55FF
!URxvt*color5  : #75507B
!URxvt*color13 : #AD7FA8
! cyan
URxvt*color6  : #06989A
URxvt*color14 : #34E2E2
! white
URxvt*color7  : #D3D7CF
URxvt*color15 : #EEEEEC


